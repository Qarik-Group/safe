#!/bin/bash

vaultVersions=(
"0.7.3"
"0.8.3"
"0.9.6"
"0.10.4"
"0.11.4"
)

if [[ $1 == "latest" ]]; then
  vaultVersions=("${vaultVersions[${#vaultVersions[@]} - 1]}")
fi

for version in "${vaultVersions[@]}"; do
  ginkgo -noisySkippings=false -p -cover -coverprofile "vaultkv${version}.coverprofile" -- -v="$version"
done
